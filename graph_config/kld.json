{
    "rcParams": {
        "font.family": "IPAPGothic",
        "font.size": 20
    },
    "subplots_args": {
        "nrows": 1,
        "ncols": 1,
        "figsize": [8, 8]
    },
    "subplots_adjust_args": {
        "wspace": 0.3
    },
    "plots":[
        {
            "title": "学習回数に対するKLDの推移",
            "column": "kl-divergence",
            "xlabel": "epoch",
            "ylabel": "KLD",
            "default_style": {
                "linewidth": 4.0
            },
            "legend_args": {
                "fontsize": 20
            }
        }
    ],
    "data-types":[
        {
            "name":"KLD",
            "typename": "meanfield_montecarlo",
            "filename_includes": "log",
            "style":{
                "linestyle": "dashed"
            }
        }
    ]
}